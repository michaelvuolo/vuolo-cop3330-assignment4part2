<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ucf.assignments.FileCreator">

    <children>
      <ToolBar layoutX="31.0" layoutY="180.0" prefHeight="40.0" prefWidth="200.0">
        <items>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="CREATE A NEW LIST..." />
        </items>
      </ToolBar>
        <HBox alignment="CENTER" layoutX="249.0" layoutY="184.0" prefHeight="33.0" prefWidth="266.0" spacing="20.0">
            <children>
                <TextField fx:id="txtNewListTitle" promptText="Title of List" />
                <Button fx:id="btnCreateList" mnemonicParsing="false" onAction="#btnCreateList" prefHeight="25.0" prefWidth="124.0" text="Create List" />
            </children>
            <opaqueInsets>
                <Insets />
            </opaqueInsets>
        </HBox>
    </children>

</AnchorPane>
